{
  "name": "dszczer/lister",
  "description": "List generator for Symfony 2.8/3.x, ready to use with Propel 2 or Doctrine 2.",
  "keywords": ["doctrine list", "propel list", "lister", "symfony list", "symfony 2 list"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Damian Szczerbi≈Ñski",
      "email": "dszczer@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/symfony": "^2.8||^3.0",
    "sensio/framework-extra-bundle": "^3.0.2||^5.0.0"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "phpdocumentor/phpdocumentor": "2.*",
    "phpunit/phpunit": "~5.7",
    "propel/propel": "~2.0@dev",
    "propel/propel-bundle": "~2.0@dev",
    "doctrine/doctrine-bundle": "~1.3",
    "doctrine/orm": "~2.5"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    },
    "psr-0": {
      "": "tests/Propel"
    }
  }
}
